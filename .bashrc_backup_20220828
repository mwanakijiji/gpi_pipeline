# .bashrc

# Source global definitions
if [ -f /etc/bashrc ]; then
	. /etc/bashrc
fi

# Uncomment the following line if you don't like systemctl's auto-paging feature:
# export SYSTEMD_PAGER=

# User specific aliases and functions

# GPI Pipeline Environment Configuration
source $HOME/.gpienv
. /home/prunelle/anaconda2/etc/profile.d/conda.sh
# added by Anaconda2 2018.12 installer
# >>> conda init >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$(CONDA_REPORT_ERRORS=false '/home/prunelle/anaconda2/bin/conda' shell.bash hook 2> /dev/null)"
if [ $? -eq 0 ]; then
    \eval "$__conda_setup"
else
    if [ -f "/home/prunelle/anaconda2/etc/profile.d/conda.sh" ]; then
        . "/home/prunelle/anaconda2/etc/profile.d/conda.sh"
        CONDA_CHANGEPS1=false conda activate base
    else
        \export PATH="/home/prunelle/anaconda2/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda init <<<
